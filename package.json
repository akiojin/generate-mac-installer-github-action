{
  "name": "@akiojin/generate-mac-installer-github-action",
  "version": "0.1.4",
  "description": "This action creates an installer (.pkg) for macOS.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm install && ncc build src/main.ts --license licenses.txt",
    "clean": "rimraf dist && rimraf node_modules && npm cache clean --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akiojin/generate-mac-installer-github-action.git"
  },
  "keywords": [
    "Unity",
    "Unity3d",
    "macOS",
    "installer"
  ],
  "author": "Akio Jinsenji <akio-jinsenji@cloud-creative-studios.com> (https://github.com/akiojin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akiojin/generate-mac-installer-github-action/issues"
  },
  "homepage": "https://github.com/akiojin/generate-mac-installer-github-action#readme",
  "dependencies": {
    "tmp": "0.2.3"
  },
  "devDependencies": {
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@akiojin/argument-builder": "1.0.0",
    "@commitlint/cli": "19.4.1",
    "@commitlint/config-conventional": "18.4.3",
    "@types/node": "20.10.6",
    "@types/tmp": "^0.2.5",
    "@vercel/ncc": "0.38.1",
    "husky": "9.1.5",
    "rimraf": "5.0.5",
    "typescript": "5.3.3"
  },
  "private": true
}
